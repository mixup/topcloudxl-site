<head>
    <meta charset="utf-8" />
    <title>{{ page.title }}</title>
    <meta name="keywords" content="{{ page.keywords }}" />
    <meta name="description" content="{{ page.description }}" />
    <meta http-equiv="content-language" content="{{ page.seoLang }}" />
    <meta property="og:image" content="{{asset('/images/facebook-og-preview.jpg')}}">

    {% if site.favicon %}
        <link rel="icon" href="{{ site.favicon['favicon'] }}" type="image/x-icon" />
        <link rel="SHORTCUT ICON" href="{{ site.favicon['thumbnail'] }}?v={{ "now"|date("U") }}" type="image/x-icon" />
    {% endif %}
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <!-- <meta name="viewport" content="width=1024"> -->
    <meta name="viewport" content="width=device-width,initial-scale=1">

    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" type="text/css" href="{{asset('/fonts/font-awesome/css/font-awesome.min.css')}}" />

    {% block templatehead %}
    {% endblock %}

    {% include basekit.headScript %}

</head>

<body class="login">

    <div class="partner-form">
        <div class="form-container">
            <img src="{{asset('images/logo-wht.png')}}">
            <div class="main-form">
                <h2><span>Login</span></h2>
                {{widget('userlogin','userlogin',{})|raw}}
            </div>
        </div>
    </div>

</body>

</html>
